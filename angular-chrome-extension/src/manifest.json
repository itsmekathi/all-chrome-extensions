{
    "name": "Angular Extension Demo",
    "version": "1.0",
    "description": "Build an Extension with Angular 10",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "webNavigation"
    ],
    "background": {
        "scripts": [
            "background.js",
            "runtime.js"
        ]
    },
    "page_action": {
        "default_popup": "index.html",
        "default_icon": "assets/favicon.png"
    }
}